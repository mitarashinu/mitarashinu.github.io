{
  "name": "mitarashinu.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:elm": "elm-live --port 55301 --start-page=index.dev.html -- src/Main.elm --output dev-dist/app.js --debug",
    "watch:sass": "node-sass -rw style --source-map true -o ../dist/stylesheets",
    "build:elm": "elm make src/Main.elm --output dist/app.js",
    "build:sass": "node-sass --omit-source-map-url -r --output-style compressed style/app.scss ../dist/stylesheets/app.css",
    "build": "npm-run-all -p build:elm build:sass",
    "watch": "npm-run-all -p watch:elm watch:sass",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mitarashinu/mitarashinu.github.io.git"
  },
  "author": "",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/mitarashinu/mitarashinu.github.io/issues"
  },
  "homepage": "https://github.com/mitarashinu/mitarashinu.github.io#readme",
  "devDependencies": {
    "elm": "^0.19.0-bugfix6",
    "elm-format": "^0.8.1",
    "elm-live": "^3.4.0",
    "elm-test": "^0.19.0-rev4",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "yarn": "^1.13.0"
  }
}
